              <Param value="CC#7|Ch6" type="fader" name="Level_6" minimum="0" maximum="127" orientation="vertical" size="3"/>
              <Param value="CC#7|Ch7" type="fader" name="Level_7" minimum="0" maximum="127" orientation="vertical" size="3"/>
              <Param value="CC#7|Ch8" type="fader" name="Level_8" minimum="0" maximum="127" orientation="vertical" size="3"/>
            </Group>

            <Group name="Master" orientation="horizontal">
              <Param value="CC#14|Ch1" type="fader" name="Level_1" minimum="0" maximum="127" orientation="vertical" size="3"/>
            </Group>
          </Row>
        </Column>

        <Column>
          <Row>
            <Group name="BPM">
              <Param value="E-7|Ch1" type="button" name="BPMDec" minimum="0" maximum="127"/>
              <Param value="F-7|Ch1" type="button" name="BPMInc" minimum="0" maximum="127"/>
            </Group>
          </Row>

          <Row>
            <Group name="EffectName">
              <Param type="label" name="DeviceName" size="4" aspect="0.25"/>
            </Group>
          </Row>

          <Row>
            <Group name="EffectParameters" columns="4">
              <Param value="CC#16|Ch1" type="dial" name="EffectParameter_1" minimum="0" maximum="127"/>
              <Param value="CC#17|Ch1" type="dial" name="EffectParameter_1" minimum="0" maximum="127"/>
              <Param value="CC#18|Ch1" type="dial" name="EffectParameter_1" minimum="0" maximum="127"/>
              <Param value="CC#19|Ch1" type="dial" name="EffectParameter_1" minimum="0" maximum="127"/>
              <Param value="CC#20|Ch1" type="dial" name="EffectParameter_1" minimum="0" maximum="127"/>
              <Param value="CC#21|Ch1" type="dial" name="EffectParameter_1" minimum="0" maximum="127"/>
              <Param value="CC#22|Ch1" type="dial" name="EffectParameter_1" minimum="0" maximum="127"/>
              <Param value="CC#23|Ch1" type="dial" name="EffectParameter_1" minimum="0" maximum="127"/>
            </Group>
          </Row>

