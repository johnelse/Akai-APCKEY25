              <Param value="A-0|Ch1" type="button" name="Slot_6_3" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="A#0|Ch1" type="button" name="Slot_7_3" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="B-0|Ch1" type="button" name="Slot_8_3" minimum="0" maximum="127" aspect="0.5"/>

              <Param value="G#-1|Ch1" type="button" name="Slot_1_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="A--1|Ch1" type="button" name="Slot_2_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="A#-1|Ch1" type="button" name="Slot_3_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="B--1|Ch1" type="button" name="Slot_4_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="C-0|Ch1"  type="button" name="Slot_5_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="C#0|Ch1"  type="button" name="Slot_6_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="D-0|Ch1"  type="button" name="Slot_7_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="D#0|Ch1"  type="button" name="Slot_8_4" minimum="0" maximum="127" aspect="0.5"/>

              <Param value="C--1|Ch1" type="button" name="Slot_1_5" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="C#-1|Ch1" type="button" name="Slot_2_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="D--1|Ch1" type="button" name="Slot_3_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="D#-1|Ch1" type="button" name="Slot_4_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="E--1|Ch1" type="button" name="Slot_5_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="F--1|Ch1" type="button" name="Slot_6_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="F#-1|Ch1" type="button" name="Slot_7_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="G--1|Ch1" type="button" name="Slot_8_4" minimum="0" maximum="127" aspect="0.5"/>
            </Group>

            <Group name="Triggers" orientation="vertical">
              <Param value="A#5|Ch1" type="button" name="Trigger_1" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="B-5|Ch1" type="button" name="Trigger_2" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="C-6|Ch1" type="button" name="Trigger_3" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="C#6|Ch1" type="button" name="Trigger_4" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="D-6|Ch1" type="button" name="Trigger_5" minimum="0" maximum="127" aspect="0.5"/>
            </Group>
          </Row>

          <Row>
            <Group name="SelectTrack" orientation="horizontal">
              <Param value="E-3|Ch1" type="button" name="TrackSelect_1" minimum="0" maximum="127"/>
              <Param value="E-3|Ch2" type="button" name="TrackSelect_2" minimum="0" maximum="127"/>
