              <Param value="B-1|Ch1" type="button" name="Slot_4_1" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="C-2|Ch1" type="button" name="Slot_5_1" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="C#2|Ch1" type="button" name="Slot_6_1" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="D-2|Ch1" type="button" name="Slot_7_1" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="D#2|Ch1" type="button" name="Slot_8_1" minimum="0" maximum="127" aspect="0.5"/>

              <Param value="C-1|Ch1" type="button" name="Slot_1_2" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="C#1|Ch1" type="button" name="Slot_2_2" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="D-1|Ch1" type="button" name="Slot_3_2" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="D#1|Ch1" type="button" name="Slot_4_2" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="E-1|Ch1" type="button" name="Slot_5_2" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="F-1|Ch1" type="button" name="Slot_6_2" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="F#1|Ch1" type="button" name="Slot_7_2" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="G-1|Ch1" type="button" name="Slot_8_2" minimum="0" maximum="127" aspect="0.5"/>

              <Param value="E-0|Ch1" type="button" name="Slot_1_3" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="F-0|Ch1" type="button" name="Slot_2_3" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="F#0|Ch1" type="button" name="Slot_3_3" minimum="0" maximum="127" aspect="0.5"/>
              <Param value="G-0|Ch1" type="button" name="Slot_4_3" minimum="0" maximum="127" aspect="0.5"/>
