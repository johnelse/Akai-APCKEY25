              <Param value="C#4|Ch1" type="pushbutton" name="NextBank"   minimum="0" maximum="127"/>
            </Group>
          </Row>

          <Row>
            <Group name="Bank">
              <Param value="C#7|Ch1" type="button" name="Left"  minimum="0" maximum="127"/>
              <Param value="A#6|Ch1" type="button" name="Up"    minimum="0" maximum="127"/>
              <Param value="B-6|Ch1" type="button" name="Down"  minimum="0" maximum="127"/>
              <Param value="C-7|Ch1" type="button" name="Right" minimum="0" maximum="127"/>
            </Group>
          </Row>
        </Column>
      </Row>
    </Page>
  </Parameters>
</Device>
