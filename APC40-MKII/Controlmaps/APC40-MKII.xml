            <Group name="EffectNavigation" columns="4">
              <Param value="A#3|Ch1" type="pushbutton" name="PrevDevice" minimum="0" maximum="127"/>
              <Param value="B-3|Ch1" type="pushbutton" name="NextDevice" minimum="0" maximum="127"/>
