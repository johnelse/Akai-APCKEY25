              <Param value="CC#54|Ch1" type="dial" name="Pan_7" minimum="0" maximum="127"/>
              <Param value="CC#55|Ch1" type="dial" name="Pan_8" minimum="0" maximum="127"/>
            </Group>
          </Row>

          <Row>
            <Group name="Slots" orientation="horizontal" columns="8">
              <Param value="G#1|Ch1" type="button" name="Slot_1_1" minimum="0" maximum="127" aspect="0.5"/>
