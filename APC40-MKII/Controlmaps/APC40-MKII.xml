              <Param value="E-3|Ch7" type="button" name="TrackSelect_7" minimum="0" maximum="127"/>
              <Param value="E-3|Ch8" type="button" name="TrackSelect_8" minimum="0" maximum="127"/>
            </Group>

            <Group name="SelectMaster">
              <Param value="A-4|Ch1" type="button" Name="SelectMaster" minimum="0" maximum="127"/>
            </Group>
          </Row>

          <Row>
            <Group name="Levels" orientation="horizontal">
              <Param value="CC#7|Ch1" type="fader" name="Level_1" minimum="0" maximum="127" orientation="vertical" size="3"/>
              <Param value="CC#7|Ch2" type="fader" name="Level_2" minimum="0" maximum="127" orientation="vertical" size="3"/>
              <Param value="CC#7|Ch3" type="fader" name="Level_3" minimum="0" maximum="127" orientation="vertical" size="3"/>
