              <Param value="CC#48|Ch1" type="dial" name="Pan_1" minimum="0" maximum="127"/>
              <Param value="CC#49|Ch1" type="dial" name="Pan_2" minimum="0" maximum="127"/>
              <Param value="CC#50|Ch1" type="dial" name="Pan_3" minimum="0" maximum="127"/>
              <Param value="CC#51|Ch1" type="dial" name="Pan_4" minimum="0" maximum="127"/>
